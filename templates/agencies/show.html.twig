{% extends 'base.html.twig' %}

{% block title %}Agencies{% endblock %}

{% block body %}
    <h1>Agencies</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ agency.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ agency.Name }}</td>
            </tr>
            <tr>
                <th>Enable</th>
                <td>{{ agency.Enable ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('agencies_index') }}">back to list</a>

    <a href="{{ path('agencies_edit', {'id': agency.id}) }}">edit</a>

    {{ include('agencies/_delete_form.html.twig') }}
{% endblock %}
