{% extends 'base.html.twig' %}

{% block title %}OrderBack index{% endblock %}

{% block body %}
    <h1>OrderBack index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>Status</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for order_back in order_backs %}
            <tr>
                <td>{{ order_back.id }}</td>
                <td>{{ order_back.Name }}</td>
                <td>{{ order_back.Status ? 'Yes' : 'No' }}</td>
                <td>
                    <a href="{{ path('order_back_show', {'id': order_back.id}) }}">show</a>
                    <a href="{{ path('order_back_edit', {'id': order_back.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="4">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('order_back_new') }}">Create new</a>
{% endblock %}
